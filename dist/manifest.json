{
  "manifest_version": 2,
  "name": "Exchange rate",
  "description": "Exchange rate indicators",
  "version": "0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "http://*.rbc.ru/",
    "https://*.rbc.ru/",
    "https://*.amazonaws.com/",
    "alarms",
    "storage"
  ]
}
